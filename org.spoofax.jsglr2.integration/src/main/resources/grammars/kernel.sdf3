module kernel

context-free start-symbols
	Start

context-free syntax
	Start = String

syntax
	String = <QMLex-LEX> <StringLex-LEX> <QMLex-LEX>  {ast("String(<2>)")}

  lexical syntax
    QMLex      = "\"" 
    StringLex  = StringChar*
    StringChar = ~[\"\n]
    StringChar = "\\\""

  lexical restrictions
    StringLex -/- ~[\"\n]